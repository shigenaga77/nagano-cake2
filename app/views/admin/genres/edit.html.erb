<h2>ジャンル編集</h2>
<%= form_with model: @genre, url: admin_genre_path do |f| %>
                
    <div class="form-group">
        <%= f.label :name %><br>
        <%= f.text_field :name %>
    </div>
    
    <div class="form-group">
        <%= f.submit 'Create Book', class: 'btn btn-success' %>
    </div>
<% end %>